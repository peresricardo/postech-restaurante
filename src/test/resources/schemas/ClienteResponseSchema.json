{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Cliente Response",
  "type": "object",
  "additionalProperties": false,

  "required": [
    "id",
    "nome",
    "email",
    "fone",
    "endereco"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "nome": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "fone": {
      "type": "string"
    },
    "endereco": {
      "type": "object",
      "required": [
        "logradouro",
        "numero",
        "complemento",
        "bairro",
        "cidade",
        "uf",
        "cep"
      ],
      "properties": {
        "logradouro": {
          "type": "string"
        },
        "numero": {
          "type": "string"
        },
        "complemento": {
          "type": "string"
        },
        "bairro": {
          "type": "string"
        },
        "cidade": {
          "type": "string"
        },
        "uf": {
          "type": "string"
        },
        "cep": {
          "type": "string"
        }
      }
    }
  }
}
